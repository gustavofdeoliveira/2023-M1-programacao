datasource db {
    provider = "sqlite"
    url      = "file:database.db"
}

generator db {
    provider  = "prisma-client-py"
    interface = "sync"
}

model Coordinate {
    id          Int      @id @default(autoincrement())
    coordinateX Float
    coordinateY Float
    coordinateZ Float
    coordinateR Float
    createdAt   DateTime @default(now())
}

